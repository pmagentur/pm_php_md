{
    "problemMatcher": [
      {
        "owner": "phpDoctor",
        "severity": "error",
        "pattern": [
          {
            "regexp": "([^/]+[\\w+].php)\\s+\\([0-9]\\s+(error)",
            "file": 1,
            "severity": 2
          },
          {
            "regexp": "^\\[([0-9]+)\\]:(.*)$",
            "line": 1,
            "message": 2,
            "loop": true
          }
        ]
      }
    ]
  }
  